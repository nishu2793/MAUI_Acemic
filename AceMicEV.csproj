	</ItemGroup>

	<ItemGroup>
	  <Compile Update="Views\OrderPage.xaml.cs">
	    <DependentUpon>OrderPage.xaml</DependentUpon>
	  </Compile>
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0-android'">
	  <PackageReference Include="BarcodeScanner.Mobile.Maui">
	    <Version>6.3.0.23-pre</Version>
	  </PackageReference>
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0-ios'">
	  <PackageReference Include="BarcodeScanner.Mobile.Maui">
	    <Version>6.3.0.23-pre</Version>
	  </PackageReference>
	</ItemGroup>
	
	<ItemGroup Condition="$(TargetFramework.StartsWith('net6.0-maccatalyst')) != true">
		<Compile Remove="**\**\*.MacCatalyst.cs" />
		<None Include="**\**\*.MacCatalyst.cs" Exclude="$(DefaultItemExcludes);$(DefaultExcludesInProjectFolder)" />
	</ItemGroup>

	<ProjectExtensions><VisualStudio><UserProperties XamarinHotReloadDebuggerTimeoutExceptionAceMicEVHideInfoBar="True" XamarinHotReloadGenericExceptionInfoBarAceMicEVHideInfoBar="True" /></VisualStudio></ProjectExtensions>

</Project>
