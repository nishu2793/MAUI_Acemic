	  <None Remove="Resources\Images\payment.png" />
	  <None Remove="Resources\Images\scan.png" />
	  <None Remove="Resources\Images\twitter.png" />
	  <None Remove="Resources\Images\usertypeimg.png" />
	  <None Remove="Resources\Images\v1_120.png" />
	  <None Remove="Resources\Images\v1_131.png" />
	  <None Remove="Resources\Images\v1_132.png" />
	  <None Remove="Resources\Images\v1_135.png" />
	  <None Remove="Resources\Images\v1_136.png" />
	  <None Remove="Resources\Images\v1_140.png" />
	  <None Remove="Resources\Images\welcome3.png" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="CommunityToolkit.Mvvm" Version="8.1.0-preview1" />
	  <PackageReference Include="DocumentFormat.OpenXml" Version="2.18.0" />
	  <PackageReference Include="Microsoft.Toolkit.Mvvm" Version="7.1.2" />
	  <PackageReference Include="Microsoft.VisualStudio.Utilities" Version="17.4.33103.184" />
	  <PackageReference Include="MongoDB.Bson" Version="2.18.0" />
	  <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
	  <PackageReference Include="sqlite-net-pcl" Version="1.8.116" />
	  <PackageReference Include="Xamarin.Firebase.iOS.Core" Version="8.10.0.3" />
	  <PackageReference Include="ZXing.Net.Maui" Version="0.1.0-preview.5" />
	</ItemGroup>

