<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AceMicEV.Views.TabbarPage"
             xmlns:local="clr-namespace:AceMicEV.Views"
             Shell.NavBarIsVisible="False"
            Shell.FlyoutBehavior="Flyout"
            Shell.FlyoutItemIsVisible="True"
             Title="TabbarPage"
            >
    
    <TabBar FlyoutItemIsVisible="False">

        <Tab Title="Home" Icon="homeicon.png">
            <ShellContent ContentTemplate="{DataTemplate local:DashBoardPage}"></ShellContent>
        </Tab>
        <Tab Title="Map" Icon="mapicon.png">
            <ShellContent ContentTemplate="{DataTemplate local:MapPage}"></ShellContent>
        </Tab>
        <Tab Title="About" Icon="about.png">
            <ShellContent ContentTemplate="{DataTemplate local:AboutPage}"></ShellContent>
        </Tab>
        <Tab Title="Booking" Icon="book.png">
            <ShellContent ContentTemplate="{DataTemplate local:SettingPage}"></ShellContent>
        </Tab>
    </TabBar>
</Shell>
    